<?xml version="1.0"?>
<package xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:xsd="http://www.w3.org/2001/XMLSchema">
	<metadata xmlns="http://schemas.microsoft.com/packaging/2010/07/nuspec.xsd">
		<id>NMoneys.Serialization.Json_NET</id>
		<title>NMoneys Serialization: Json.NET</title>
		<version>2.0.0.0</version>
		<authors>Daniel González García</authors>
		<licenseUrl>http://www.opensource.org/licenses/bsd-license.php</licenseUrl>
		<requireLicenseAcceptance>false</requireLicenseAcceptance>
		<description>Custom serialiation when NMoneys objects are to be serialized/deserialized using Json.NET.</description>
		<summary>
			NMoneys Serialization: Json.NET is an extension to the NMoneys library that allows customize the serialization and deserialization process
			when using the library Json.NET via custom converters.
		</summary>
		<projectUrl>http://code.google.com/p/nmoneys/</projectUrl>
		<iconUrl>https://nugetgallery.blob.core.windows.net/icons/NMoneys.2.0.0.0.png</iconUrl>
		<tags>.net currency money</tags>
		<dependencies>
			<dependency id="nmoneys" version="3.1.0.0" />
		</dependencies>
	</metadata>
	<files>
		<file src="content\**\CanonicalMoneyConverter.cs" target="content" />
		<file src="content\**\DefaultMoneyConverter.cs" target="content" />
		<file src="content\**\CurrencyLessMoneyConverter.cs" target="content" />
		<file src="content\**\CurrencyStyle.cs" target="content" />
		<file src="content\**\MoneyOperator.cs" target="content" />
		<file src="content\**\SupportForReading.cs" target="content" />
		<file src="content\**\SupportForWriting.cs" target="content" />
	</files>
</package>